{
  "name": "example-e2e",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/test-e2e/src",
  "projectType": "application",
  "tags": [],
  "targets": {
    "clean": {},
    "build": {
      "executor": "@nx/js:tsc",
      "dependsOn": ["action:build"],
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "packages/test-e2e/dist",
        "main": "packages/test-e2e/src/index.ts",
        "tsConfig": "packages/test-e2e/tsconfig.lib.json",
        "assets": [
          "packages/test-e2e/*.md",
          {
            "glob": "**/*",
            "input": "packages/test-e2e/src/generated",
            "output": "src/generated"
          }
        ]
      }
    },
    "test": {
      "command": "echo 'Test target disabled for this project - only \"test-e2e\" is supported'"
    },
    "ensure-ability-policy-built-and-deployed-to-ipfs": {
      "dependsOn": ["ability-native-send:action:deploy", "policy-counter:action:deploy", "^build"]
    },
    "test-e2e": {
      "dependsOn": ["ensure-ability-policy-built-and-deployed-to-ipfs"],
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/test-output/jest/{projectRoot}"],
      "options": {
        "jestConfig": "{projectRoot}/jest.config.js"
      }
    }
  }
}
